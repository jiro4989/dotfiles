[init]
	templatedir = ~/.git_template
[user]
	email = jiroron666@gmail.com
	name = jiro4989
[alias]
	preq = pull-request
	see = browse
	fpr = "!f(){ git fetch origin pull/$1/head:$1; git checkout $1; };f"
	co = checkout
[commit]
	template = ~/dotfiles/conf/misc/gitcommit.template
	gpgsign = true
[ghq]
	root = ~/src
[pull]
	ff = only
